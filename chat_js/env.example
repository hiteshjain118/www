# Chat JS Server Environment Variables

# Server Settings
PORT=3004
NODE_ENV=development

# CORS Settings
CORS_ORIGIN=http://localhost:3002,http://localhost:3001

# WebSocket Settings
WS_HOST=localhost
WS_PORT=3004

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# QuickBooks Configuration
QBO_CLIENT_ID=your_qbo_client_id_here
QBO_CLIENT_SECRET=your_qbo_client_secret_here

# Session Management
SESSION_TIMEOUT=3600000
MAX_SESSIONS=1000

# Logging
LOG_LEVEL=info
LOG_FILE=logs/chat-server.log

# Database Configuration
DATABASE_URL=your_database_url_here

# Supabase Configuration (Required for SupabaseStorageService)
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Intent Server Configuration
INTENT_SERVER_ENABLED=true
DEFAULT_INTENT=qb

# Security
JWT_SECRET=dev-websocket-secret-change-in-production
RATE_LIMIT_ENABLED=true
MAX_CONNECTIONS_PER_IP=10 